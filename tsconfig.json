{
    "compilerOptions": {
      "target": "es2018",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": [
        "esnext",
        "esnext.asynciterable",
        "dom"
      ],
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": "./",
      "paths": {
        "~/*": [
          "src/*"
        ],
        "@/*": [
          "src/*"
        ]
      },
      "types": [
        "node",
        "@types/node",
        "@nuxt/types",
        "vuetify",
      ],
      "experimentalDecorators": true,
    },
    "exclude": [
      "node_modules"
    ]
  }